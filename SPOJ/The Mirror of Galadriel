#include<stdio.h>
#include<string.h>

int main()
{int t;
  scanf("%d",&t);
   while(t--)
    {char str[15];int g=0;
      scanf("%s",&str);
        for(int i=0,k=strlen(str)-1;str[i]!='\0';i++,k--)
         {if(str[i]!=str[k])
           {g=1;break;}
         }
          if(g==1)
           printf("NO\n");
            else 
             printf("YES\n");
    }
     return 0;
}
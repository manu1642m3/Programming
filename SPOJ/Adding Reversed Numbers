#include<stdio.h>
int main()
{long int a,b,t,q,w,e;
 scanf("%ld",&t);
 
  while(t--)
  {long int c=0;
   scanf("%ld",&a);
   scanf("%ld",&b);
 
    while(a>=1)
     {c=c*10+a%10;
     a=a/10;
    }
     a=0;
     while(b>=1)
      {a=a*10+b%10;
       b=b/10;
      }
 
      c=c+a;
      a=0;
      while(c>=1)
       {a=a*10+c%10;
        c=c/10;
       }
       printf("\n%ld",a);
 
      }
      return(0);
 
    }  
 



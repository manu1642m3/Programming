#include<stdio.h>
int main()
{int a,ex[40]={0,0,0,0,1,1,1,1,6,2,4,8,1,3,9,7,6,4,6,4,5,5,5,5,6,6,6,6,1,7,9,3,6,8,4,2,1,9,1,9},t;
long int b;
scanf("%d",&t);

while(t--)
 {scanf("%d",&a);
  scanf("%ld",&b);
  a=a%10;
  if(b==0)
   {printf("\n1");continue;}
   
   b=b%4;
     
   a=ex[b+a*4];
   
   
  printf("\n%d",a); 
  
 }
 
 return(0);
}
   





